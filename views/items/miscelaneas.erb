<table id='tresp'>
	<thead>
		<tr>
			<th>Icono</th>
			<th>Nombre</th>
			<th>Pieza</th>
			<th>Ranuras</th>
			<th>Precio</th>
		</tr>	
	</thead>
	<tbody>
		<% miscelaneas.each do |misc| %>
		<% m = Miscelanea.new(misc)%>
			<tr>
				<td><img src=<%= m.img_path %> alt=<%= m.name %>, id='icon' /></td>
				<td><%= m.name %></td>
				<td><%= m.fits.capitalize %></td>
				<td><%= m.max %></td>
				<td><%= m.precio %></td>
			</tr>
		<% end %>
	</tbody>
</table>